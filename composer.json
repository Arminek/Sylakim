{
    "name": "sylake/sylakim-bundle",
    "type": "symfony-bundle",
    "description": "Export your Akeneo PIM catalog to Sylius.",
    "keywords": ["sylake", "sylakim", "pim", "akeneo", "sylius", "akeneopim", "akeneo-pim", "ecommerce", "e-commerce", "connector", "catalog", "products"],
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Janvier",
            "email": "jjanvier@gmail.com"
        },
        {
            "name": "Arnaud Langlade",
            "email": "arn0d.dev@gmail.com"
        },
        {
            "name": "Romain Monceau",
            "email": "monceau.romain@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Lakion/sylius-api-php.git"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "akeneo/pim-community-dev": "~1.6",
        "lakion/sylius-api-php": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Sylake\\Bundle\\SylakimBundle\\": "src/" }
    }
}
